# Arrow.dev Server Environment Configuration
# Copy this file to .env and fill in your values

# Google Gemini API Key (Required)
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_api_key_here

# Server Port (Optional, default: 5000)
PORT=5000

# Node Environment (Optional, default: development)
# Values: development, production, test
NODE_ENV=development

# Frontend URL for CORS (Optional, default: http://localhost:5173)
# Required if your frontend is running on a different domain
FRONTEND_URL=http://localhost:5173

# Example configuration:
# GEMINI_API_KEY=AIzaSyYourActualApiKeyHere
# PORT=5000
# NODE_ENV=development
# FRONTEND_URL=http://localhost:3000

OPENROUTER_API_KEY=openrouter_api_key